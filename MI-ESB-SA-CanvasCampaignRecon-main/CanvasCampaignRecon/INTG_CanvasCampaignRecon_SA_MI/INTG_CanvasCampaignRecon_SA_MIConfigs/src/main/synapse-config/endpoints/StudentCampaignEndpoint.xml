<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="StudentCampaignEndpoint" xmlns="http://ws.apache.org/ns/synapse">
    <http method="get" trace="enable" uri-template="https://wso2test.free.beeceptor.com/campaigndata">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
